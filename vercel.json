{
    "version": 2,
    "builds": [
        {
            "src": "app.js",
            "use": "@vercel/node"
        },
        {
            "src": "public/**",
            "use": "@vercel/static"
        }
    ],
    "routes": [
        {
            "src": "/test.html",
            "dest": "/public/test.html"
        },
        {
            "src": "/index.html",
            "dest": "/public/index.html"
        },
        {
            "src": "/api/(.*)",
            "dest": "/app.js"
        },
        {
            "src": "/",
            "dest": "/public/index.html"
        }
    ]
}